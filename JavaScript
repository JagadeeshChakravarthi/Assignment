"use strict"
let num1 = 10
let num2 = 5

let addResult = num1 + num2
let subResult = num1 - num2
let divResult = num1 / num2
let multResult = num1 * num2

console.log(`
             Addition : ${addResult} ,
             Subtraction : ${subResult},
             Division : ${divResult},
             Multipliction : ${multResult}
             `)


             
